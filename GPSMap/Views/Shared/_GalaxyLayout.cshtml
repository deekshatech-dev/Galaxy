    <script>
        function ShowOverlay() {
            $("#overlay").show();
            $("#spinner").show();

        }
        function HideOverlay() {
            $("#overlay").hide();
            $("#spinner").hide();
        }
        $(document).ready(function () {
            toastr.options = {
                "closeButton": true,
                "debug": false,
                "newestOnTop": false,
                "progressBar": false,
                "positionClass": "toast-top-center",
                "preventDuplicates": false,
                "onclick": null,
                "showDuration": "300",
                "hideDuration": "1000",
                "timeOut": "5000",
                "extendedTimeOut": "1000",
                "showEasing": "swing",
                "hideEasing": "linear",
                "showMethod": "fadeIn",
                "hideMethod": "fadeOut",
                "preventDuplicates": true,
                "preventOpenDuplicates": true
            };
        });
    </script>
    <script type="text/javascript">
        $(document).ready(() => {
            var statusValue = $("#status").text();
            if (statusValue != null && statusValue != "") {
                if (statusValue == "Duplicate") {
                    toastr.error("Record already exists");
                }
                if (statusValue == "Failed") {
                    toastr.error("Some error occured. Please try again");
                }
                if (statusValue == "Created") {
                    toastr.success("Record has been created");
                }
                if (statusValue == "Updated") {
                    toastr.success("Record has been udpated");
                }

                if (statusValue == "Deleted") {
                    toastr.success("Record has been deleted");
                }
                if (statusValue == "DeleteError") {
                    toastr.success("Record cannot be deleted. Please try again or contact administrator.");
                }
            }
        });
    </script>
