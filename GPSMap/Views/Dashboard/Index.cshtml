@using GPSMap.Data;
@model GPSMap.Models.SearchViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-md-3">
        <div class="treeview-animated border mx-4 my-4">
            @*<h3 class="pt-3 pl-3" style="text-align:center;">Public Directories</h3>*@
            @*<hr>*@
            <ul class="treeview-animated-list mb-3">
                <li class="treeview-animated-items">
                    <a class="closed">
                        <i class="fas fa-angle-right"></i>
                        <span>GEO</span>
                    </a>
                    <ul class="nested">
                        <li>
                            Air Interface
                        </li>
                        <li>
                            Quality of Services
                        </li>
                        <li>
                            Single User
                        </li>
                        <li>
                            Traffic Density
                        </li>
                    </ul>
                </li>
                <li class="treeview-animated-items">
                    <a class="closed">
                        <i class="fas fa-angle-right"></i>
                        <span>Reports</span>
                    </a>
                    <ul class="nested">
                        <li>
                            Dashboard
                        </li>
                        <li>
                            Level 1 Top Performance Indicators
                        </li>
                        <li>
                            Level 1 Root Cause Analysis
                        </li>
                        <li>
                            Level 3 Troubleshooting
                        </li>
                        <li>
                            single User
                        </li>
                    </ul>
